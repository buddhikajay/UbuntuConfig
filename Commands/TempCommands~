sudo chmod go-rwx /var/www/html/TubeSite/web
sudo chmod go+x /var/www/html/TubeSite/web
sudo chgrp -R www-data /var/www/html/TubeSite/web
sudo chmod -R go-rwx /var/www/html/TubeSite/web
sudo chmod -R g+rx /var/www/html/TubeSite/web
sudo chmod -R g+rwx /var/www/html/TubeSite/web

